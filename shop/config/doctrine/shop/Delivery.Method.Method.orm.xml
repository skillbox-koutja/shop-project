<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Model\Shop\Entity\Delivery\Method\Method" table="shop_delivery_methods">
    <id name="id" type="shop_delivery_method_id" column="id"/>
    <field name="title" type="string" column="title" nullable="false"/>
    <field name="type" type="shop_delivery_method_type" column="type" nullable="false"/>
    <field name="minPrice" type="integer" column="min_price" nullable="true"/>
    <field name="cost" type="integer" column="cost" nullable="true"/>
  </entity>
</doctrine-mapping>
